{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "",
      "camelCase"
    ],
    "max-line-length": [
      true,
      1400
    ],
    "component-selector": [
      true,
      "element",
      "coop",
      "kebab-case"
    ],
    "indent": [
      true,
      "tabs"
    ]
  }
}
